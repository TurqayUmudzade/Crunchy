        </button>
        <div class="collapse navbar-collapse " id="navbarNav ">
            <ul class="navbar-nav ">
                <li class="nav-item active ">
                    <a class="nav-link " href="@Url.Action("Index","Home",new { area="Admin"})">Home <span class="sr-only ">(current)</span></a>
                </li>
                <li class="nav-item ">
                    <a class="nav-link " href="@Url.Action("Index","Classes",new { area="Admin"})">Classes</a>
                </li>
