                    <a class="nav-link " href="@Url.Action("Trainers","Trainer",new { area="Admin"})">Trainers</a>
                </li>
                <li class="nav-item ">
                    <a class="nav-link " href="@Url.Action("Gyms","Gym",new { area="Admin"})">Gyms</a>
                </li>
            </ul>
        </div>
    </nav>
    @RenderBody()

   
