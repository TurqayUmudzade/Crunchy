@{ 
    //layout with user name on nav
}
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- MY CSS -->
    <link rel="stylesheet" href="~/css/main.css">
    <!-- FONT AWESOME -->
    <script src="https://kit.fontawesome.com/e3658e79d2.js" crossorigin="anonymous"></script>
    <!-- GOOGLE FONTS -->
    <link href="https://fonts.googleapis.com/css?family=Roboto+Mono&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Roboto+Condensed&display=swap" rel="stylesheet">
    <!-- JQUERY -->
    <script src="https://code.jquery.com/jquery-3.4.1.min.js" integrity="sha256-CSXorXvZcTkaix6Yvo6HppcZGetbYMGWSFlBw8HfCJo=" crossorigin="anonymous"></script>
    <!-- Chart -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.6.0/Chart.min.js"></script>
    <!--ReCapture-->
    <script src="https://www.google.com/recaptcha/api.js" async defer></script>
    <!--MemberAreaLayoutJS-->
    <script src="~/js/member-area.js"></script>
    <title>@ViewData["Title"]</title>
</head>

<body>

    <nav>
        <div class="flex">
            <a href="" id="logo">
                <img class="menu-bar__logo" alt="Crunch Logo" src="https://d2l0v8ee8pblzo.cloudfront.net/assets/core/logo-f0321dadbee0b6b8ec6d98159305c76e56590ff97a88d28d4eb2400b3baafe49.svg">
            </a>
            <a class="nav-anchor" href="" id="1">LOCATIONS</a>
            <a class="nav-anchor" href="" id="2">CLASSES</a>
            <a class="nav-anchor" href="" id="3">HIITZONE</a>
            <a class="nav-anchor" href="" id="4">TRANING</a>
            <a class="nav-anchor" href="" id="5">ABOUT</a>
            <i class="fas fa-bars"></i>
            <div class="lang_picker flex">
                <img class="flag" src="https://dj7w0h47bhjwk.cloudfront.net/assets/core/flags/en-07772f1861e8e765c83be31bb1af70a23ecf038fc73041705d6f67f26ae4521a.png" alt="En">
                <h2>EN</h2>
                <i class="fas fa-sort-down"></i>
                <ul class="chooser">
                    <li class="opened">
                        <a>
                            <img class="flag" src="https://dj7w0h47bhjwk.cloudfront.net/assets/core/flags/en-07772f1861e8e765c83be31bb1af70a23ecf038fc73041705d6f67f26ae4521a.png" alt="En">
                            <p class="lang_long_name">United States</p>
                            <p class="arrow"></p>
                        </a>
                        <ul class="sub_chooser">
                            <li>
                                <a href="/about?locale=en">
                                    <p class="lang_long_name">English</p>
                                </a>
                            </li>
                            <li>
                                <a href="/about?locale=es">
                                    <p class="lang_long_name">Spanish</p>
                                </a>
                            </li>
                        </ul>
                    </li>
                    <li>
                        <a href="https://www.crunch.com.au/" target="_blank">
                            <img class="flag" src="https://dj7w0h47bhjwk.cloudfront.net/assets/core/flags/en-AU-11a08e93915063e28b9bf32ea439cce104d97c8b5c809f012656950d750b2337.png" alt="En au">
                            <p class="lang_long_name">Australia</p>
                        </a>
                    </li>
                    <li>
                        <a href="https://www.crunchfitness.ca" target="_blank">
                            <img class="flag" src="https://dj7w0h47bhjwk.cloudfront.net/assets/core/flags/en-CA-a47ac4281a264334d74288c85c70de4bb1f7cb6439f5b2156bcde5e365b44fd7.png" alt="En ca">
                            <p class="lang_long_name">Canada</p>
                        </a>
                    </li>
                    <li>
                        <a>
                            <img class="flag" src="https://dj7w0h47bhjwk.cloudfront.net/assets/core/flags/es-cr-ba792778a97c8236d38a8d164c5bee2c10e439c12b0a977ce454f56896e3ab02.png" alt="Es cr">
                            <p class="lang_long_name">Costa Rica</p>
                            <p class="arrow"></p>
                        </a>
                        <ul class="sub_chooser">
                            <li>
                                <a href="/about?locale=es-cr">
                                    <p class="lang_long_name">Spanish</p>
                                </a>
                            </li>
                        </ul>
                    </li>
                    <li>
                        <a href="https://cr7crunch.fitness/es" target="_blank">
                            <img class="flag" src="https://dj7w0h47bhjwk.cloudfront.net/assets/core/flags/es-8b91bd57e960c9078897020e6e0b09db748c562f96849a91e29d079f95143e49.png" alt="Es">
                            <p class="lang_long_name">Spain</p>
                        </a>
                    </li>
                </ul>
            </div>
            <a id="free-trial" href="@Url.ActionLink("Home","MyGym")">@Model.firstName</a>
            <a id="join" href="@Url.ActionLink("Logout","Account")">Log out</a>
        </div>
    </nav>

    <section id="member-area">
        <div class="container">
            <div class="nav-pane">
                <div class="component left-navigation">
                    <a href="#!" class="nav-sub-menu-trigger title">
                        <h2>In this section</h2>
                    </a>
                    <ul class="nav-menu closed-mobile">
                        <li class="expandable ">
                            <a href="#" class="nav-sub-menu-trigger not-a-link">
                                <span>My gym</span>
                            </a>
                            <ul class="nav-sub-menu">
                                <li>
                                    <a href=@Url.ActionLink("Home","MyGym")>
                                        <span>Home</span>
                                    </a>
                                </li>
                                <li>
                                    <a href=@Url.ActionLink("MyGymUsage","MyGym")>
                                        <span>My gym usage</span>
                                    </a>
                                </li>

                                <li>
                                    <a href=@Url.ActionLink("AverageUsage","MyGym")>
                                        <span>Average usage chart</span>
                                    </a>
                                </li>
                                <li>
                                    <a href=@Url.ActionLink("Contact","MyGym")>
                                        <span>Member contact us</span>
                                    </a>
                                </li>

                            </ul>
                        </li>
                        <li class="expandable ">
                            <a href="#" class="nav-sub-menu-trigger not-a-link">
                                <span>My classes and PTs</span>
                            </a>
                            <ul class="nav-sub-menu">
                               
                                <li>
                                    <a href=@Url.ActionLink("BookClass","MemberArea")>
                                        <span>Book a class</span>
                                    </a>
                                </li>
                                <li>
                                    <a href=@Url.ActionLink("MyBookings","MemberArea")>
                                        <span>My bookings</span>
                                    </a>
                                </li>
                                <li>
                                    <a href=@Url.ActionLink("Trainers","MemberArea")>
                                        <span>Personal trainers</span>
                                    </a>
                                </li>
                            </ul>
                        </li>
                        <li class="expandable ">
                            <a href="#" class="nav-sub-menu-trigger not-a-link">
                                <span>My membership</span>
                            </a>
                            <ul class="nav-sub-menu">
                                <li>
                                    <a href=@Url.ActionLink("ChangeHomeGym","MyMembership")>
                                        <span>Change home gym</span>
                                    </a>
                                </li>

                                <li>
                                    <a href=@Url.ActionLink("FreezeMemberShip","MyMembership")>
                                        <span>Freeze membership</span>
                                    </a>
                                </li>
                                <li>
                                    <a href=@Url.ActionLink("CancelMemberShip","MyMembership")>
                                        <span>Cancel membership</span>
                                    </a>
                                </li>
                                <li>
                                    <a href=@Url.ActionLink("Parking","MyMembership")>
                                        <span>Manage parking</span>
                                    </a>
                                </li>

                            </ul>
                        </li>
                        <li class="expandable ">
                            <a href="#" class="nav-sub-menu-trigger not-a-link">
                                <span>My account</span>
                            </a>
                            <ul class="nav-sub-menu">
                                <li>
                                    <a href="/member-area/my-account/change-details/">
                                        <span>Change details</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="/member-area/my-account/change-direct-debit/">
                                        <span>Change direct debit</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="/member-area/my-account/view-payment-history/">
                                        <span>View payment history</span>
                                    </a>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </div>
            @RenderBody()
        </div>
    </section>
    <footer>
        <div class=" footer-wrapper flex ">
            <div class=" left ">
                <div class=" icons ">
                    <i class=" fab fa-facebook-f "></i>
                    <i class=" fab fa-instagram "></i>
                    <i class=" fab fa-twitter "></i>
                    <i class=" fab fa-youtube "></i>
                </div>
                <p>© CRUNCH FITNESS</p>
                <p id=" grey ">ALL RIGHTS RESERVED</p>
            </div>
            <div class=" right ">
                <p>FREE TRIAL</p>
                <p id=" grey ">
                    OWN A CRUNCH • CONTACT • FAQ • CORPORATE MEMBERSHIPS • CAREERS • TERMS OF USE • PRIVACY • POLICY
                </p>
            </div>

        </div>
    </footer>

    <script src="~/js/navbar.js"></script>
    <script src="~/js/login.js"></script>
    <script src="~/js/member-area.js"></script>
</body>


</html>

